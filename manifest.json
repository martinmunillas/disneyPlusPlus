{
  "name": "DisneyPlusPlus",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.disneyplus.com/*"],
      "js": ["build/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "DisneyPlusPlus"
  },
  "permissions": ["storage"]
}
